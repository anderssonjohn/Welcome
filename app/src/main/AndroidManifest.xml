<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dat255.chalmers.com.welcome">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ChatActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LanguageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MentorChoiceActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".GenderAndBirthActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".JobSvActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".JobAsActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".InfoSvActivity" />
        <activity android:name=".InfoAsActivity" />
        <activity
            android:name=".ProfileSettingsActivity"
            android:label="@string/title_activity_profile_settings"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="dat255.chalmers.com.welcome.MainActivity" />
        </activity>
    </application>

</manifest>